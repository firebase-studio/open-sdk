{
  "name": "@firebase-studio/open-sdk",
  "version": "0.1.0",
  "type": "module",
  "description": "A TypeScript SDK for opening workspaces in Firebase Studio from a browser.",
  "keywords": [
    "firebase",
    "firebase-studio",
    "idx",
    "project-idx"
  ],
  "author": "Google LLC",
  "license": "Apache-2.0",
  "source": "src/index.ts",
  "main": "dist/index.cjs",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "microbundle --no-sourcemap ./src/index.ts",
    "dev": "npm run build -- -w"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "esbuild": "^0.20.2",
    "microbundle": "^0.15.1",
    "typescript": ">=5.2.2"
  }
}
